        </div>
        <div class="col-xs-8 col-sm-4 col-md-4">
          <div class="align-center txt-shadow">
            <%#= image_tag 'f.png', :height => '150', :class => 'img-rakuten' %>
            <span class="color-white">I'm photographer</span>
          </div>
        </div>
      </div>
    </div>
  </section-->
  <!-- /Section: separator -->

  <!-- Section: gallery -->
  <section id="instagram" class="home-section text-left bg-instagram">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
          <div class="wow bounceInDown" data-wow-delay="0.4s">
            <div class="section-heading" class="navbar-toggle" data-toggle="collapse" data-target="#ghost_area_instagram">
              <!--h2><%#= image_tag 'i.png', :height => '50', :class => 'img-rakuten' %>INSTAGRAM</h2-->
              <%= image_tag 'i.png', :height => '60', :class => 'img-rakuten' %><p>INSTAGRAM</p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="container collapse" id="ghost_area_instagram">
      <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12" >
          <div class="wow bounceInUp" data-wow-delay="0.4s">
            <div id="instagram-owl-works" class="owl-carousel"></div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- /Section: services -->

  <!-- Section: gallery -->
  <section id="rakuten" class="home-section text-left bg-rakuten">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
          <div class="wow bounceInDown" data-wow-delay="0.4s">
            <div class="section-heading" class="navbar-toggle" data-toggle="collapse" data-target="#ghost_area_rakuten">
              <!--h2><%#= image_tag 'r.jpg', :height => '50', :class => 'img-rakuten' %>RAKUTEN</h2-->
              <%= image_tag 'r.jpg', :height => '60', :class => 'img-rakuten' %><p>RAKUTEN</p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="container collapse" id="ghost_area_rakuten">
      <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12" >
          <div class="wow bounceInUp" data-wow-delay="0.4s">
            <div id="rakuten-owl-works" class="owl-carousel"></div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- /Section: services -->
  

<section  class="home-section text-left bg-flickr">

  <!-- Section: gallery -->
    <div class="container" id="flickr">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
          <div class="wow bounceInDown" data-wow-delay="0.4s">
            <div class="section-heading" class="navbar-toggle" data-toggle="collapse" data-target="#ghost_area_flickr">
              <!--h2><%#= image_tag 'f.png', :height => '50', :class => 'img-rakuten' %>FLICKR</h2-->
              <%= image_tag 'f.png', :height => '60', :class => 'img-rakuten' %><p>FLICKR</p>
            </div>
          </div>
